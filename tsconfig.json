{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "jsx": "react",
    "lib": ["es5", "es6", "es7", "dom", "es2017.object"],
    "module": "es6",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "./build/",
    "target": "es5",
    "downlevelIteration": true,
    "skipLibCheck": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": ["node_modules"]
}
